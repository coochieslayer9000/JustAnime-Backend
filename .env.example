# Origins you want to allow, use * to allow all origins (not recommended for production)
ALLOWED_ORIGINS=*

# Port the server will run on [OPTIONAL - default: 4444]
PORT=4444

# AniList OAuth Configuration (for OAuth proxy endpoints)
ANILIST_CLIENT_ID=<your_anilist_client_id>
ANILIST_CLIENT_SECRET=<your_anilist_client_secret>

# MyAnimeList OAuth Configuration (for OAuth proxy endpoints)
MAL_CLIENT_ID=<your_mal_client_id>
MAL_CLIENT_SECRET=<your_mal_client_secret>
